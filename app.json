{
  "expo": {
    "name": "Andr√© Lanches",
    "slug": "andrelanches",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/andre.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "image": "./assets/images/screensplash.png",
    "resizeMode": "contain",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/andre.png",
        "backgroundColor": "#d30000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/andree.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/screensplash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d30000"
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/screensplash.png",
      "backgroundColor": "#ff0000"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
